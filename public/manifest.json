{
  "manifest_version": 3,
  "name": "Click KE",
  "version": "1.0",
  
  "background": {
    "service_worker": "background.js"
    
  },
 
  "side_panel": {
    "default_path": "index.html"
  },
  "icons": {
    "16": "./images/icon_16.png",
    "32": "./images/icon_32.png",
    "48": "./images/icon_48.png",
    "128": "./images/icon_128.png"
  },
  "permissions": ["sidePanel", "storage"]
}
